<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:descendantFocusability="blocksDescendants"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <CheckBox
        android:id="@+id/checkbox_towview"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:gravity="center" />

    <ImageView
        android:id="@+id/image_towview"
        android:layout_width="80dp"
        android:layout_height="80dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/name_towview"
            android:text="66"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/tiem_towview"
            android:text="66"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/price_towview"
                android:text="66"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <com.bw.xuhongtao0306.view.widget.JaJianQi
                android:id="@+id/jiajianqi_towview"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_alignParentRight="true"></com.bw.xuhongtao0306.view.widget.JaJianQi>


        </RelativeLayout>


    </LinearLayout>
</LinearLayout>